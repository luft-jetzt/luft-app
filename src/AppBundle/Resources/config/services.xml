<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="services/admin.xml"/>
    </imports>

    <services>
        <defaults autowire="true" autoconfigure="true" public="true" />

        <service id="AppBundle\SourceFetcher\Persister\Persister" />
        <service id="AppBundle\Pollution\PollutionDataFactory\PollutionDataFactory">
            <argument type="service" id="doctrine" />
            <argument type="service" id="fos_elastica.finder.air.station" />
        </service>
    </services>
</container>
